@model AllProductsViewModel

<link rel="stylesheet" href="~/css/index.css" asp-append-version="true" />


<form method="get">
    <div class="row-mb-5">
        <div class="form-group col-md-4">
            <div class="form-group">
                <label asp-for="Brand"></label>
                <select asp-for="Brand" class="form-control">

                    @foreach (var brand in Model.Brands)
                    {
                        <option value=@brand>@brand</option>
                    }
                </select>
            </div>
        </div>
    </div>

    <div class="row-mb-5">
        <div class="form-group col-md-4">
            <div class="form-group">
                <label asp-for="Type"></label>
                <select asp-for="Type" class="form-control">

                    @foreach (var type in Model.Types)
                    {
                        <option value=@type>@type</option>
                    }
                </select>
            </div>
        </div>
    </div>

    <div class="row-mb-5">
        <div class="form-group col-md-4">
            <div class="form-group">
                <label asp-for="SortedBy"></label>
                <select asp-for="SortedBy" class="form-control">
                    <option value="0">Sorting By Price</option>
                    <option value="1">Sorting By Price Descending</option>
                    <option value="2">Sorting By Name</option>
                    <option value="3">Sorting By Most Buys</option>
                </select>
            </div>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Search</button>


</form>

<br />
<br />
<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <div class="panel panel-danger">
                <div class="panel-heading">SALE % OFF!</div>
                <div class="panel-body"><img src="data:image/jpeg;base64," class="img-responsive" style="width:100%" alt="Image"></div>
                <div class="panel-footer">Don't waste time buy <button type="button" class="btn btn-danger" onclick="location.href='@Url.Action("SaleProducts", "Shop")'">NOW</button> </div>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="panel panel-primary">
                <div class="panel-heading">Most buyed products</div>
                <div class="panel-body"><img src="https://cdn.shopify.com/s/files/1/0063/4218/0928/products/5-Core_ZMA_Tier_WEB-90caps_600x.png?v=1646069044" class="img-responsive"  style="width:100%" alt="Image"></div>
                <div class="panel-footer">Check our most buyed products in shop <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("MostBuyedProducts", "Shop")'">Check</button></div>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="panel panel-success">
                <div class="panel-heading">All products</div>
                <div class="panel-body"><img src="https://2.imimg.com/data2/XL/QJ/MY-1438739/sport-nutrition-supplements-250x250.jpg" class="img-responsive" style="width:100%" alt="Image"></div>
                <div class="panel-footer">See our all products <button type="button" class="btn btn-success" onclick="location.href='@Url.Action("AllProducts", "Shop")'">Shop NOW</button></div>
            </div>
        </div>
    </div>
</div>
<br>